{
  "version": 2,
  "builds": [
    {
      "src": "tokenxllm/dashboard/frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },

    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
